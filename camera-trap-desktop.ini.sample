[Folders]


[SQLite]
dbfile = camera-trap-desktop.db

[Thumbnail]
destination = camera-trap-desktop
prefix = thumb-

[AnnotationFieldSpecies]
type = select
mutiple = 1
choices = 山羌:a|山羊:b|水鹿:c
